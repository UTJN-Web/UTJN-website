generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                Int      @id @default(autoincrement())
  lastName         String
  firstName        String
  email             String   @unique
  major             String
  graduationYear   Int
  cognitoSub        String   @unique
  joinedAt         DateTime @default(now())
}
